{
  "name": "electron-updater-example",
  "version": "0.4.3",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "^19.4.2"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^1.4.2"
  },
  "scripts": {
    "start": "electron main.js",
    "build-win": "build --win -p always",
    "dist": "build",
    "htt": "http-server Hello/ -p 8080"
  },
  "build": {
    "appId": "com.github.iffy.electronupdaterexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
